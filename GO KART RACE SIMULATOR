local Library = loadstring(Game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))()
local Window = Library:NewWindow("GO KART RACE SIMULATOR")

local Tab = Window:NewSection("Main")

Tab:CreateTextbox("Give Speed Op", function(text)

        local args = {
    [1] = "Train",
    [2] = "Power",
    [3] = (text)
}

game:GetService("ReplicatedStorage").Remotes.ClientRemote:InvokeServer(unpack(args))

end)

Tab:CreateToggle("Give Win", function(value)

     Toggle = value;
end)

spawn(function()
     while wait(0.000001) do
               if Toggle then
               local args = {
    [1] = 6
}

game:GetService("ReplicatedStorage").Remotes.Spin:FireServer(unpack(args))
     end
end

end)

