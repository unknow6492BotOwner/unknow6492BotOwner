local Library = loadstring(Game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))()
local Window = Library:NewWindow("BaconScript#9172 HUB")
local Tab = Window:NewSection("Main")

Tab:CreateTextbox("Get Money", function(text)

local args = {
    [1] = "Buy",
    [2] = "WoodKatana",
    [3] = -(text)
}

game:GetService("ReplicatedStorage").Remotes.Swords:FireServer(unpack(args))
        
end)

Tab:CreateToggle("Infinity Win", function(value)

     Toggle = value;
end)
spawn(function()
     while wait() do
               if Toggle then
               local args = {
    [1] = "G5"
}

game:GetService("ReplicatedStorage").Remotes.Gift:FireServer(unpack(args))

     end
end

end

local Tab = Window:NewSection("GamepassSword")

Tab:CreateButton("Give Omega Sword", function()

local args = {
    [1] = "G7"
}

game:GetService("ReplicatedStorage").Remotes.Gift:FireServer(unpack(args))

end)

