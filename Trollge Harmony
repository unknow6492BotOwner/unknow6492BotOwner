Tab:CreateTextbox("Jumppower Set", function(text)

        game.Players.LocalPlayer.Character.Humanoid.JumpPower = (text)

end)

Tab:CreateTextbox("Gravity", function(text)

        workspace.Gravity = (text)

end)

